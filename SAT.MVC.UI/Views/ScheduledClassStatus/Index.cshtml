@model IEnumerable<SAT.MVC.EF.ScheduledClassStatus>

@{
    ViewBag.Title = "Scheduled Class Status";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SCSName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SCSName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SSCID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SSCID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SSCID })
        </td>
    </tr>
}

</table>
